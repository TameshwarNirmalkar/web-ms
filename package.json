{
  "name": "web-ms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 --configuration=dev && node --max-old-space-size=4096",
    "build": "ng build && node --max-old-space-size=4096",
    "prod-build-login": "npm run pre-build-login && node --max-old-space-size=2042 ./node_modules/@angular/cli/bin/ng build --app=1 --prod --aot --ec=true --vendor-chunk=false && npm run login-gulp",
    "build-login": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --app=1 --prod --aot --ec=true --stats-json --vendor-chunk=false",
    "prod-build": "npm run pre-build && node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --prod --aot --ec=true && gulp",
    "test-build": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --prod --aot --ec=true --stats-json",
    "serve": "ng serve -c dev --poll=2000 && node --max-old-space-size=4096",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "bundle-report-solitaire": "webpack-bundle-analyzer dist/solitaire-app/stats.json",
    "bundle-report-login": "webpack-bundle-analyzer dist/login-app/stats.json",
    "pre-build": "gulp --gulpfile pre-build-tasks.js",
    "pre-build-login": "gulp --gulpfile pre-build-login-tasks.js",
    "login-gulp": "gulp --gulpfile ./login-gulp.js",
    "build-apps": "npm run prod-build && npm run prod-build-login && npm run precache"
  },
  "private": true,
